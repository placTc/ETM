
alphabet:
  blank: "-"
  symbols: ["-", 0, 1, "#"]
  input: [0, 1]

tape:
  initial_tape: [blank, 0, 1, 0, 0, 0, 0, "#"]
  initial_index: 0

state:
  initial: q_init
  halting: halt
  null_move: true
  states:
    q_init:
      default:
        move: R
      "#":
        transition: q_dec
        move: L
    q_dec:
      0:
        write: 1
        move: L
      1:
        write: 0
        move: L
        transition: halt
      default:
        transition: halt
    halt: null